# ドメイン設定
DOMAIN=example.com

# Let's Encrypt設定
LETSENCRYPT_EMAIL=your-email@example.com

# Traefik管理画面
TRAEFIK_USER=admin
# パスワードハッシュの生成方法: htpasswd -nb admin your-password | sed -e s/\\$/\\$\\$/g
TRAEFIK_PASSWORD_HASH=$$apr1$$your-hash-here

# Nextcloudデータベース
NEXTCLOUD_DB_ROOT_PASSWORD=secure-root-password
NEXTCLOUD_DB_PASSWORD=secure-db-password

# Nextcloud管理者
NEXTCLOUD_ADMIN_USER=admin
NEXTCLOUD_ADMIN_PASSWORD=secure-admin-password

# Redmineデータベース
REDMINE_DB_PASSWORD=secure-redmine-password

# Redmineシークレットキー（以下のコマンドで生成: openssl rand -hex 64）
REDMINE_SECRET_KEY=your-secret-key-here